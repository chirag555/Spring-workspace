ackage webapp.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
@Entity
@Table(name = "posts")
public class test {

	@Id
	@Column(name = "id")
	int postId;
	@Column(name = "title")
	String title;
	@Column(name = "body")
	String body;
	
	public test() {

	}

	public test(int postId, String title, String body, Author author) {

		this.postId = postId;
		this.title = title;
		this.body = body;
		//this.author = author;
	}

	
}
